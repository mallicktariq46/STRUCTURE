#include<iostream>
using namespace std;

struct cube { //Creating structure
    double height=2.0;
    double width=4.0;
    double length=6.0;
    
};

int main(){
cube cube1; //To access the elements of the structure
int vol=cube1.height*cube1.length*cube1.width;

cout<<"Volume of the cube is: "<<vol;
return 0;
}

/*OUTPUT
Volume of the cube is: 48
*/
